{
  "name": "winston-google-analytics",
  "version": "1.0.0",
  "author": "Anders Janmyr <anders@janmyr.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:andersjanmyr/winston-google-analytics.git"
  },
  "main": "winston-google-analytics.js",
  "scripts": {
    "test": "mocha --reporter spec test",
    "test-int": "mocha --reporter spec --timeout 20000 test",
    "test-watch": "mocha --watch test json --reporter min",
    "test-debug": "mocha test --reporter spec --debug-brk",
    "test-ci": "mocha test --reporter xunit > ./tmp/test-results.xml",
    "jshint": "jshint **/*.js",
    "jshint-ci": "jshint --reporter checkstyle **/*.js > ./tmp/jshint.xml"
  },
  "jshintConfig": {
    "bitwise": true,
    "camelcase": true,
    "curly": false,
    "expr": true,
    "eqeqeq": true,
    "immed": true,
    "indent": 4,
    "latedef": "nofunct",
    "newcap": true,
    "undef": true,
    "unused": true,
    "strict": true,
    "globalstrict": true,
    "trailing": true,
    "maxparams": 4,
    "maxdepth": 2,
    "maxcomplexity": 6,
    "node": true,
    "browser": true,
    "globals": {
      "describe": true,
      "before": true,
      "it": true
    }
  },
  "dependencies": {
    "debug": "^2.0.0",
    "universal-analytics": "^0.3.4",
    "winston": "^0.7.3"
  },
  "devDependencies": {
    "jshint": "",
    "mocha": "",
    "chai": "",
    "sinon": "",
    "supervisor": ""
  }
}
